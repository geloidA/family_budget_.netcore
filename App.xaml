<Application x:Class="family_budget.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:family_budget">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="IconsDictionary"
                                    Source="pack://application:,,,/family_budget;component/Resourses/Icons.xaml"/>
                <ResourceDictionary x:Name="StylesDictionary"
                                    Source="pack://application:,,,/family_budget;component/Resourses/Styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
